{
  "name": "ng-helpers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod ng-helpers",
    "postbuild": "cp ./README.md ./dist/@jaspero/ng-helpers && cp LICENSE ./dist/@jaspero/ng-helpers",
    "test": "ls",
    "test:app": "ls",
    "lint": "ng lint ng-helpers",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "Angular",
    "Utilites",
    "Helpers"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.{ts,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "8.0.0-rc.4",
    "@angular/common": "8.0.0-rc.4",
    "@angular/compiler": "8.0.0-rc.4",
    "@angular/core": "8.0.0-rc.4",
    "@angular/forms": "8.0.0-rc.4",
    "@angular/platform-browser": "8.0.0-rc.4",
    "@angular/platform-browser-dynamic": "8.0.0-rc.4",
    "@angular/router": "8.0.0-rc.4",
    "@jaspero/ng-helpers": "1.3.1",
    "rxjs": "6.5.2",
    "tslib": "1.9.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.0-rc.3",
    "@angular-devkit/build-ng-packagr": "0.800.0-rc.4",
    "@angular/cli": "8.0.0-rc.4",
    "@angular/compiler-cli": "8.0.0-rc.4",
    "@angular/language-service": "8.0.0-rc.4",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.0.2",
    "codelyzer": "5.0.1",
    "husky": "2.3.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "8.1.7",
    "ng-packagr": "5.2.0",
    "prettier": "1.17.1",
    "protractor": "5.4.2",
    "semantic-release": "^15.13.14",
    "ts-node": "8.1.0",
    "tsickle": ">=0.29.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  },
  "release": {
    "pkgRoot": "dist/@jaspero/ng-helpers",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
