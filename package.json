    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "16.0.0",
    "prettier": "2.8.2",
    "protractor": "7.0.0",
    "semantic-release": "20.0.2",
    "ts-node": "10.9.1",
    "tsickle": "0.46.3",
    "tslint": "6.1.3",
    "typescript": "5.0.4"
